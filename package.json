{
  "name": "algorithm-template",
  "displayName": "Algorithm Template",
  "description": "自动插入灵神算法模板代码片段",
  "version": "0.1.2",
  "publisher": "liyunhua",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/algorithmTemplate.git"
  },
  "license": "MIT",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Snippets"
  ],
  "activationEvents": [
    "onLanguage:go"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "go",
        "aliases": [
          "Go",
          "go"
        ],
        "extensions": [
          ".go"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "generate": "node scripts/generate-templates.js",
    "compile": "npm run generate && tsc -p ./",
    "watch": "npm run generate && tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}

